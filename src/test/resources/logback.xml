<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">

    <appender name="console"
              class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%p] %d{yyyy-MM-dd HH:mm:ss.S} [%t] [%c] %m%n %ex{full}
            </pattern>
        </encoder>
    </appender>

    <root level="DEBUG">
        <appender-ref ref="console"/>
    </root>
</configuration>
